{
    "manifest_version": 2,
    "name": "Cute.Fasti | URL Shortener",
    "version": "0.1.1",
    "description": "Shortens the current URL and copies the shortened URL to the clipboard",
    "permissions": ["activeTab", "clipboardWrite", "notifications", "https://www.kii.su/*"],
    "browser_action": {
      "default_icon": "icons/default.png"
    },
    "icons": {
      "32": "icons/default.png"
    },
    "background": {
      "scripts": ["background.js"]
    }
  }